<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rongji.dfish.framework.plugin.lob.entity.PubLob" table="PUB_LOB">
        <id name="lobId" type="java.lang.String">
            <column name="LOB_ID"/>
            <generator class="assigned" />
        </id>
        <property name="lobContent" type="java.lang.String">
            <column name="LOB_CONTENT"></column>
        </property>
        <property name="operTime" type="java.util.Date">
            <column name="OPER_TIME"></column>
        </property>
        <property name="archiveFlag" type="java.lang.String">
            <column name="ARCHIVE_FLAG"></column>
        </property>
        <property name="archiveTime" type="java.util.Date">
            <column name="ARCHIVE_TIME"></column>
        </property>
    </class>
</hibernate-mapping>
